<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <settings>
        <!--输出日志-->
        <setting name="logImpl" value="STDOUT_LOGGING"/>
    </settings>

    <!--设置类型的别名-->
    <typeAliases>
        <!--type：全限定名称-->
        <!-- alias：别名-->
        <typeAlias type="org.xiao.rain.vo.user.UserVo" alias="uservo"/>
        <package name="org.xiao.rain.vo.corp"/>
    </typeAliases>
    
    <mappers>
<!--        <mapper resource="org/xiao/rain/dao/itf/IRainQueryDao.xml"/>-->
        <package name="org.xiao.rain.dao.itf"/>
    </mappers>
</configuration>